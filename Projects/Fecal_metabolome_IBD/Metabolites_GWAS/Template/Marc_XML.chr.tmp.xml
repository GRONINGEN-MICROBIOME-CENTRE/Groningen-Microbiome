<?xml version="1.0" encoding="utf-8" standalone="no"?>
<settings>
        <defaults>
                <qc>
                        <snpqccallratethreshold>0.0</snpqccallratethreshold>
                        <snpqchwethreshold>0.0</snpqchwethreshold>
                        <snpqcmafthreshold>0.05</snpqcmafthreshold>
                </qc>

                <analysis>
                        <analysistype>trans</analysistype>
                        <cisanalysisprobedistance>1</cisanalysisprobedistance>
                        <correlationtype>nonparametric</correlationtype>
                        <regressOutEQTLEffects/>
                        <threads>5</threads>
                        <onlypermutations>false</onlypermutations>
                        <createdotplot>false</createdotplot>
                        <createqqplot>false</createqqplot>
                        <TwoPartModel>CONTINUES</TwoPartModel>
                </analysis>

                <multipletesting>
                        <type>fdr</type>
                        <threshold>0.05</threshold>
                        <permutations>0</permutations>
                </multipletesting>

                <output>
                        <outputdirectory>final_out/</outputdirectory>
                        <outputplotthreshold>0.0</outputplotthreshold>
                        <outputplotdirectory>final_out/</outputplotdirectory>
                        <maxnreqtlresults>800000000</maxnreqtlresults>
                        <generatesnpsummarystatistics>true</generatesnpsummarystatistics>
                        <generateeqtlpvaluetable>true</generateeqtlpvaluetable>
                </output>

                <confine>
                        <snp></snp>
                        <snpProbe></snpProbe>
                        <probe></probe>
                        <confineSNPsToSNPsPresentInAllDatasets>false</confineSNPsToSNPsPresentInAllDatasets>
                        <confineSNPsSelectSNPInStrongestLD>false</confineSNPsSelectSNPInStrongestLD>
                        <confineProbesThatMapToKnownChromosome>true</confineProbesThatMapToKnownChromosome>
                </confine>
        </defaults>
<datasets>
                <dataset>
                        <name>LLD</name>
                        <location>lld_genetic_input</location>
                        <genometoexpressioncoupling>lld_coupling</genometoexpressioncoupling>
                        <expressiondata>lld_trait</expressiondata>
                        <probeannotation>lld_annot</probeannotation>
                        <expressionplatform>RDP</expressionplatform>
                        <quantilenormalize>false</quantilenormalize>
                        <logtranform>false</logtranform>
                </dataset>
                <dataset>
                        <name>CD</name>
                        <location>cd_genetic_input</location>
                        <genometoexpressioncoupling>cd_coupling</genometoexpressioncoupling>
                        <expressiondata>cd_trait</expressiondata>
                        <probeannotation>cd_annot</probeannotation>
                        <expressionplatform>RDP</expressionplatform>
                        <quantilenormalize>false</quantilenormalize>
                        <logtranform>false</logtranform>
                </dataset>
<dataset>
                        <name>UC</name>
                        <location>uc_genetic_input</location>
                        <genometoexpressioncoupling>uc_coupling</genometoexpressioncoupling>
                        <expressiondata>uc_trait</expressiondata>
                        <probeannotation>uc_annot</probeannotation>
                        <expressionplatform>RDP</expressionplatform>
                        <quantilenormalize>false</quantilenormalize>
                        <logtranform>false</logtranform>
                </dataset>
        </datasets>
</settings>
